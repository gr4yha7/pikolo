# Mezo Testnet (Matsnet) Deployment Configuration
# Copy this file to .env and fill in your values

# Private key for deployment (without 0x prefix)
# IMPORTANT: Never commit this file to git!
MATSNET_PRIVATE_KEY=your_private_key_here

# Optional: Explorer API key for contract verification
MEZO_EXPLORER_API_KEY=your_explorer_api_key_here

# Optional: RPC URLs (if not using default)
# SEPOLIA_RPC_URL=https://sepolia.infura.io/v3/YOUR_KEY
# MAINNET_RPC_URL=https://mainnet.infura.io/v3/YOUR_KEY
